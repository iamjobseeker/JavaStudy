package Study;

import java.util.Scanner;

public class Roadrunner_황진경 {

	public static void main(String[] args) {

		//	Scanner kb=new Scanner(System.in);
		//	String drt=kb.next(); //한줄에 입력하기

		int ms=10;
		int location = 5;

		for(int i=1;i<ms;i++) {

			if(i==location){
				System.out.print("*");
			}

			else {System.out.print("-");
			}
		}

	}

}


